{
    "version": 2,
    "functions": {
        "api/**/*.php": {
            "runtime": "vercel-php@0.7.0"
        }
    },
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/api/index.php"
        }
    ],
    "outputDirectory": "public",
    "buildCommand": "npm run build"
}